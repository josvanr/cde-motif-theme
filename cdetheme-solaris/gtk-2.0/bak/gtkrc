		    overlay_stretch = FALSE
		    arrow_direction = RIGHT
		}    
	} #engine pixmap
} #style scrollbar


style "styleRange"
{
	GtkRange::slider_width = 13
	#what  determines the length?? The png seems to be stretched in lenght 
	#a couple of pixels. 
	engine "pixmap" {
		image {
			function = BOX
			detail = "trough"
			#file = "scale/trough-horiz.png"
			file = "img/scrollbarTrough.png"
			#border = {3, 3, 9, 9}
            		border = {3, 3, 3, 3}
			stretch	= TRUE
			orientation	= HORIZONTAL
		}
		image {
			function = BOX
			detail = "trough"
			#file = "scale/trough-vert.png"
			file = "img/scrollbarTrough.png"
            		border = {3, 3, 3, 3}
			#border = {9, 9, 3, 3}
			stretch	= TRUE
			orientation	= VERTICAL
		}
		image {
			function = SLIDER
			#overlay_file = "scale/slider-horiz.png"
			file = "img/rangeSliderHorizontal.png"
			stretch	= TRUE
			orientation	= HORIZONTAL
		}
		image {
			function = SLIDER
			#overlay_file = "scale/slider-vert.png"
			file = "img/rangeSliderVertical.png"
			#overlay_stretch	= FALSE
			orientation	= VERTICAL
		}
		#aha this is the focus line around widget . Was disabled by setting to null.
		#In CDE this is orange so keep (set in "default" style)
		#image {
			#function = FOCUS
			#file = "null.png"
		#} 
	}
}


style "styleNotebook" {
	#thickness leads to the selected notebook getting drawn larger.. (aha?)
	xthickness = 2
	ythickness = 2
  	GtkNotebook::tab-overlap = 4

	#some pixel trouble in this department but how to solve...?
	engine "pixmap" 
	{
		#this seems to be unnecessary (or defined also elswhere)
		image {
			function = BOX
			file = "img/notebook.png"
			border = {1, 1, 1, 1}
			stretch	= TRUE
		      }
		image {
			function = EXTENSION
			state = ACTIVE
			file = "img/notebookTabTop.png"
			border = {1, 1, 1, 1}
			stretch	= TRUE
			gap_side = BOTTOM
		}
		image {
			function = EXTENSION
			file = "img/notebookTabTopActive.png"
			border = {1, 1, 1, 1}
			stretch = TRUE
			gap_side = BOTTOM
		}
		image {
			function = EXTENSION
			state = ACTIVE
			file = "img/notebookTabBottom.png"
			border = {1, 1, 1, 1}
			stretch	= TRUE
			gap_side = TOP
		}
		image {
			function = EXTENSION
			file = "img/notebookTabBottomActive.png"
			border = {1, 1, 1, 1}
			stretch	= TRUE
			gap_side = TOP
		}
		image {
			function = EXTENSION
			state = ACTIVE
			file = "img/notebookTabLeft.png"
			border = {1,1,1,1}
			stretch	= TRUE
			gap_side = RIGHT
		}
		image {
			function = EXTENSION
			file = "img/notebookTabLeftActive.png"
			border = {1, 1, 1, 1}
			stretch = TRUE
			gap_side = RIGHT
		}
    		image {
			function = EXTENSION
			state = ACTIVE
			file = "img/notebookTabRight.png"
			border = {1, 1, 1, 1}
			stretch	= TRUE
			gap_side = LEFT
		}
		image {
			function = EXTENSION
			file = "img/notebookTabRightActive.png"
			border = {1, 1, 1, 1}
			stretch	= TRUE
			gap_side = LEFT
		}
		#note this represents open area in the box not in the tab
		#if this is not present, the entire box is drawn by default
		#drawer not by pixmap. the pixmaps in the box then have no 
		#effect
		image {
			function = BOX_GAP
			file = "img/notebook.png"
			border = {1, 1, 1, 1}
			stretch	= TRUE
			gap_file = "img/notebookTopGap.png"
			gap_border = {1, 1, 0, 0}
			gap_start_file = "null.png"
			gap_start_border = {0, 0, 0, 0}
			gap_end_file = "null.png"
			gap_end_border = {0, 0, 0, 0}
			gap_side = TOP
		}
		image {
			function = BOX_GAP
			file = "img/notebook.png"
			border = {1, 1, 1, 1}
			stretch	= TRUE
			gap_file = "img/notebookBottomGap.png"
			gap_border = {1, 1, 0, 0}
			gap_start_file = "null.png"
			gap_start_border = {0, 0, 0, 0}
			gap_end_file = "null.png"
			gap_end_border = {0, 0, 0, 0}
			gap_side = BOTTOM
		      }
		image {
			function = BOX_GAP
			file = "img/notebook.png"
			border = {1, 1, 1, 1}
			stretch	= TRUE
			gap_file = "img/notebookLeftGap.png"
			gap_border = {0, 0, 1, 1}
			gap_start_file = "null.png"
			gap_start_border = {0, 0, 0, 0}
			gap_end_file = "null.png"
			gap_end_border = {0, 0, 0, 0}
			gap_side = LEFT
      		}
		image {
			function = BOX_GAP
			file = "img/notebook.png" 
			border = {1, 1, 1, 1}
			stretch	= TRUE
			gap_file = "img/notebookRightGap.png"
